<div class="mypage edit">
  <div class="mypage1">
    <h3 class="customer-title">会員情報編集</h3>
       <%= form_with model: @customer,url: customer_path(@customer.id),method: :patch, local: true do |f|%>
       <table>
          <tr>
            <td>氏名</td>
            <td><%= f.text_field :last_name %></td>
            <td><%= f.text_field :first_name %></td>
          </tr>
          <tr>
            <td>フリガナ</td>
            <td><%= f.text_field :last_name_kana %></td>
            <td><%= f.text_field :first_name_kana %></td>
          </tr>
          <tr>
            <td>郵便番号</td>
            <td><%= f.text_field :postal_code %></td>
          </tr>
          <tr>
            <td>住所</td>
            <td><%= f.text_field :address %></td>
          </tr>
          <tr>
            <td>電話番号</td>
            <td><%= f.text_field :telephone_number %></td>
          </tr>
          <tr>
            <td>メールアドレス</td>
            <td><%= f.text_field :email %></td>
          </tr>
       </table>
       <%= f.submit'編集内容を保存' %>
       <%#= link_to "退会", customer_path(@customer.id),method: :delete %>
       <% end %>
    </div>
</div>