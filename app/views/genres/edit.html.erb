<div class="title-logo"><h3 class="title">ジャンル編集</h3></div>
<div class="genre-index">
<%= form_with model:@genre, url:genre_path(@genre.id), local:true do |f| %>
  <%= f.label :ジャンル名 %>
  <%= f.text_field :name %>
  <%= f.submit '変更を保存', class: "btn btn-outline-success genre-button" %>
<% end %>
</div>
